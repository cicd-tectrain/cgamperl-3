            steps {
                echo 'Building feature'
            }
        }

        stage('Testing Feature') {
            steps {
                echo 'Testing feature'
            }
        }

        stage('Integrating Feature') {
            steps {
                echo 'Integrating feature'
            }
        }

    }


}